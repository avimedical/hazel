{
  "name": "hazel-server",
  "version": "5.0.0",
  "main": "lib/index.js",
  "description": "Lightweight update server for Electron apps",
  "scripts": {
    "dev": "micro-dev lib/server.js",
    "test": "xo && jest",
    "precommit": "lint-staged"
  },
  "license": "MIT",
  "repository": "zeit/hazel",
  "xo": {
    "extends": [
      "prettier"
    ],
    "rules": {
      "camelcase": 0,
      "new-cap": 0,
      "unicorn/no-process-exit": 0,
      "no-await-in-loop": 0
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn test --passWithNoTests && :",
      "prettier --single-quote --no-semi --write --no-editorconfig",
      "git add"
    ]
  },
  "dependencies": {
    "@vercel/node": "1.9.0",
    "async-retry": "1.3.1",
    "date-fns": "2.17.0",
    "express-useragent": "1.0.15",
    "fetch": "1.1.0",
    "finalhandler": "1.1.2",
    "handlebars": "4.7.7",
    "jest": "26.6.3",
    "micro": "9.3.4",
    "ms": "2.1.3",
    "node-fetch": "2.6.1",
    "router": "1.3.5",
    "semver": "7.3.4",
    "stream-to-string": "1.2.0",
    "test-listen": "1.1.0",
    "vercel": "21.3.0"
  },
  "devDependencies": {
    "eslint-config-prettier": "8.1.0",
    "husky": "5.1.1",
    "lint-staged": "10.5.4",
    "micro-dev": "3.0.0",
    "npm-check-updates": "11.1.9",
    "prettier": "2.2.1",
    "xo": "0.38.1"
  }
}
